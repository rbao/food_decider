.span6.offset3
  %h1 Vote
  = form_for @vote do |f|
    %table.table.table-striped
      - @votable_restaurants.each do |restaurant|
        %tr.restaurant
          %td= check_box_tag 'vote[restaurant_ids][]', restaurant.id, f.object.restaurant_ids.include?(restaurant.id)
          %td= render 'restaurants/restaurant_td', restaurant: restaurant